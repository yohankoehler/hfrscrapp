<span class="page-current" style="display : none">{{page}}</span>

<div class="block" id="mygallery">
  {{#each imagesObj}}
    <a class="image-link" data-posthref="{{postHref}}" href="{{src}}" title="p.{{page}}" data-page="{{page}}">
      <img class="b-lazy {{#if spoiler}}spoiler{{/if}}" src="{{src}}"  />
    </a>
  {{/each}}
</div>
<button id="load-more">more...</button>
